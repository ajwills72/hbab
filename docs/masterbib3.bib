	Journal = {The Quarterly journal of experimental psychology. B, Comparative and physiological psychology},
	Keywords = {Analysis of Variance,Animals,Association Learning,Attention,Color Perception,Columbidae,Conditioning, Classical,Conditioning, Classical: physiology,Cues,Discrimination Learning,Discrimination Learning: physiology,Pattern Recognition, Visual,Pattern Recognition, Visual: physiology,Reaction Time,Reinforcement (Psychology)},
	Month = oct,
	Number = {4},
	Pages = {349--64},
